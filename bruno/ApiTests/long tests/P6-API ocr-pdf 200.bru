meta {
  name: P6-API ocr-pdf 200
  type: http
  seq: 2
}

post {
  url: {{apigw}}/ocr-pdf
  body: multipartForm
  auth: none
}

body:multipart-form {
  file: @file(..\files\hugePdf.pdf)
}

assert {
  res.status: eq 200
  res.body.length: gte 0
}

script:post-response {
  bru.setEnvVar("file_content", res.body)
}
